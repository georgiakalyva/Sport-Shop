<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Admin.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-1.7.1.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <title></title>
</head>
<body>
    <div id="header">
        <div>
            <img src="/Images/logo.png" />
            <img src="/Images/ath1.png" style="height:100px; padding-left:40px" />
            <img src="/Images/ath2.png" style="height:100px;" />
            <img src="/Images/ath3.png" style="height:100px;" />
            <img src="/Images/ath4.png" style="height:100px;" />
        </div>
    </div>
    <div>
        @if (TempData["message"] != null) {
            <div class="Message">@TempData["message"]</div>
        }
        @RenderBody()
    </div>
    <footer>
        <div align="center" style="color: white; padding-top:20px;">Sports Store Copyright 2014</div>
 </footer>

</body>
</html>